[user]
[push]
  default = matching
[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  who = shortlog -s --
  last = log -1 HEAD
  unstage = reset HEAD --
  graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  amend = commit --amend --no-edit
