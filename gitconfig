[core]
  ignorecase = false
  excludesfile = ~/.gitignore
[push]
  default = upstream
[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  who = shortlog -s --
  last = log -1 HEAD
  unstage = reset HEAD --
  graph = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(auto)%h%d %s %C(black)%C(bold)%cr'
  append = commit --amend --no-edit
  first = rev-list --max-parents=0 HEAD
[credential]
  helper = cache
[rerere]
  enabled = true
